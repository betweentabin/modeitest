{
  "rewrites": [
    {
      "source": "/storage/(.*)",
      "destination": "https://heroic-celebration-production.up.railway.app/storage/$1"
    },
    {
      "source": "/((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
