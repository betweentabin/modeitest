{
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/news/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/publications/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!js/|css/|img/|favicon.ico|_next/|api/).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
