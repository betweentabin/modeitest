{
  "name": "vue-package",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm install --legacy-peer-deps && vue-cli-service build",
    "start": "npx serve -s dist -l $PORT",
    "preinstall": "npm install --package-lock-only --legacy-peer-deps || echo 'package-lock.json generation failed'"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "core-js": "^3.6.5",
    "serve": "^14.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "vue-template-compiler": "^2.6.11"
  }
}
